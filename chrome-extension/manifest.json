{
    "name": "EmoReply",
    "version": "2.0",
    "description": "You wouldn't understand :'(",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "720": "images/Sadface.jpg"
        }
    },
    "icons": {
        "720": "images/Sadface.jpg"
    },
    "permissions": [
        "http://localhost:5000/responses"
    ]
}